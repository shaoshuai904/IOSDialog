<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/ms_act_sel_bg" />
            <corners android:radius="@dimen/ms_shape_radius" />
            <stroke
                android:width="@dimen/ms_shape_width"
                android:color="@color/ms_act_line" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/ms_act_def_bg" />
            <corners android:radius="@dimen/ms_shape_radius" />
            <stroke
                android:width="@dimen/ms_shape_width"
                android:color="@color/ms_act_line" />
        </shape>
    </item>

</selector>